{
  "name": "prettify-json",
  "displayName": "Prettify JSON",
  "description": "Visual Studio Code Prettify JSON Extension",
  "version": "0.0.5",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "git@github.com:mohsen1/vscode-prettify-json.git"
  },
  "publisher": "mohsen1",
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
	"Programming Languages",
	"Formatters"
  ],
  "activationEvents": [
    "onCommand:extension.prettifyJSON",
    "onCommand:extension.prettifyEscapedJSON"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.prettifyJSON",
        "title": "Prettify JSON"
	  },
	  {
		  "command": "extension.prettifyEscapedJSON",
		  "title": "Prettify Escaped JSON"
	  }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "dependencies": {
    "jsonlint": "^1.6.2",
    "strip-json-comments": "^2.0.0"
  }
}
